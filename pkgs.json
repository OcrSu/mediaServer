{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "php:serve": "php artisan serve",
        "php:pail": "php artisan pail",
        "php:queue": "php artisan queue:listen",
        "php:scheduler": "php artisan schedule:work",
        "vite:dev": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold\" \"npm:php:serve\" \"npm:php:pail\" \"npm:dev\"",
        "vite:cron": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold\" \"npm:php:queue\" \"npm:php:scheduler\" \"npm:dev\"",
        "build": "run-p type-check \"build-only {@}\" --",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint": "eslint . --fix",
        "lintold": "eslint ./resources/js --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier .  --write ./resources/js/**/*.{js,ts,vue,json}"
    },
    "devDependencies": {
        "@eslint/js": "^9.7.0",
        "@types/node": "^22.9.3",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@typescript-eslint/parser": "^8.13.0",
        "@tsconfig/node22": "^22.0.0",
        "@vue/eslint-config-prettier": "^10.1.0",
        "@vue/eslint-config-typescript": "^14.1.3",
        "@vue/tsconfig": "^0.7.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@iconify-json/circum": "^1.1.11",
        "@iconify-json/lucide": "^1.1.202",
        "@iconify-json/material-symbols-light": "^1.1.25",
        "@iconify-json/mdi-light": "^1.2.1",
        "@iconify-json/ph": "^1.1.13",
        "autoprefixer": "^10.4.19",
        "axios": "^1.7.4",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.30.0",
        "laravel-vite-plugin": "^1.0.0",
        "postcss": "^8.4.35",
        "prettier": "3.3.3",
        "tailwindcss": "^3.4.1",
        "unplugin-icons": "^0.19.0",
        "npm-run-all2": "^7.0.1",
        "typescript": "~5.6.3",
        "vite": "^6.0.1",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "@eslint/compat": "^1.2.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tanstack/vue-query": "^5.59.20",
        "@vueuse/components": "^10.11.0",
        "@vueuse/core": "^10.11.0",
        "@vueuse/integrations": "^11.0.0",
        "concurrently": "^9.1.0",
        "focus-trap": "^7.5.4",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isequal": "^4.5.0",
        "pinia": "^2.2.6",
        "tailwind-scrollbar-hide": "^1.1.7",
        "vue": "^3.5.13",
        "vue-loader": "^17.4.2",
        "vue-router": "^4.4.5"
    }
}
